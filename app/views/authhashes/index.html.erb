<div class="page-header">
  <h1>
    Authhashes
    <a href="/authhashes/new" class="btn btn-lg btn-success">New Authhash</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Provider</th>
        <th>Actions</th>
      </tr>

      <% @authhashes.each do |authhash| %>
      <tr>
        <td><%= authhash.provider %></td>
        <td>
          <a href="/authhashes/<%= authhash.id %>" class="btn btn-primary">Show</a>
          <a href="/authhashes/<%= authhash.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_authhash/<%= authhash.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
